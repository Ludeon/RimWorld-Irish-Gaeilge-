<?xml version="1.0" encoding="UTF-8"?>
<LanguageData>
  
  <!-- EN: Follow master while the master is drafted. -->
  <FollowDrafted.headerTip>Lean máistir nuair atá an máistir dréachtaithe.</FollowDrafted.headerTip>
  
  <!-- EN: Follow master while the master is doing field work (hunting or taming animals). -->
  <FollowFieldwork.headerTip>Lean máistir nuair atá obair ar siúl ag an máistir (sealgaireacht nó ceansú ainmhithe).</FollowFieldwork.headerTip>
  
  <!-- EN: Mark animals to be hunted by your hunters.\n\nOnly hunters with suitable ranged weapons will hunt. -->
  <Hunt.headerTip>Roghnaigh ainmhithe atá le seilg.</Hunt.headerTip>
  
  <!-- EN: Release to wild -->
  <ReleaseAnimalToWild.headerTip>Scaoil saor</ReleaseAnimalToWild.headerTip>
  
  <!-- EN: Slaughter -->
  <Slaughter.headerTip>Maraigh</Slaughter.headerTip>
  
  <!-- EN: Sterilize -->
  <Sterile.headerTip>Steiriligh</Sterile.headerTip>
  
  <!-- EN: Tame a wild animal. -->
  <Tame.headerTip>Ceansaigh ainmhí fiáin.</Tame.headerTip>
  
</LanguageData>