  <NeedIndoors.stages.outdoors.label>lasmuigh</NeedIndoors.stages.outdoors.label>
  <!-- EN: I've been outside too much recently. -->
  <NeedIndoors.stages.outdoors.description>Ba cheart go mbeidh mé faoi dhíon.</NeedIndoors.stages.outdoors.description>
