  <NeedComfort.stages.comfortable.label>compordach</NeedComfort.stages.comfortable.label>
  <!-- EN: I've been comfortable recently. -->
  <NeedComfort.stages.comfortable.description>Tá mé compordach.</NeedComfort.stages.comfortable.description>
